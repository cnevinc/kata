class Solution {
    // "" ,"" ?
    // null / null ?
    public boolean isAnagram(String s, String t) {
        int[] feq = new int[26];
        for (int i = 0; i < s.length(); i++) {
            char c = s.charAt(i);
            feq[c-'a']++;
        }
        for (int i = 0; i < t.length(); i++) {
            char c = t.charAt(i);
            feq[c-'a']--;
        }
        for (int i = 0; i < feq.length; i++) {
            if (feq[i]!=0) {
                return false;
            }
        }
        return true;
        
    }
}

